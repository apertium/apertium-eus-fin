      </program> 
      <program name="apertium-transfer -b">
        <file name="apertium-eus-fin.eus-fin.t1x"/>
        <file name="eus-fin.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-eus-fin.eus-fin.t2x"/>
